   "source": [
    "#find the sum of all items in a list.user should enter elements of the list\n",
    "a=(input(\"enter the numbers with comma seperated:\"))\n",
    "b=a.split(\",\")\n",
    "l=len(b)\n",
    "d=0\n",
    "i=0\n",
    "for i in range(l):\n",
    "     c=int(b[i])\n",
    "     d=d+c   \n",
    "print(\"sum of elements in a list \",b,\"is: \",d)\n"
   ]
